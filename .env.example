# App Store Connect API Credentials
# Get these from App Store Connect → Users and Access → Keys

# Your 10-character key ID (e.g., ABC123DEF4)
APP_STORE_KEY_ID=YOUR_KEY_ID_HERE

# Your issuer ID (UUID format)
APP_STORE_ISSUER_ID=YOUR_ISSUER_ID_HERE

# Path to your P8 private key file
APP_STORE_P8_PATH=/path/to/your/AuthKey_XXXXXXXXXX.p8

# Optional: Vendor number for financial reports
# Find in App Store Connect → Payments and Financial Reports
APP_STORE_VENDOR_NUMBER=YOUR_VENDOR_NUMBER

# Optional: Debug mode
DEBUG=false